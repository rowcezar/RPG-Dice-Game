package {	import flash.display.Sprite;	import flash.events.Event;	import flash.display.MovieClip;	public class DiceCube extends MovieClip	{		public var Luck:Number;		public var AngleRoll:Number;		public var Side:int;		public var RollMode:Boolean = false;		public var StartDelay:Boolean = false;		public var Delay:int = 0;		public var EsperandoResultado:Boolean = false;				/*private function DiceCube(){			addEventListener(MouseEvent.MOUSE_DOWN, Roll);		}*/		public function Roll()		{			StartDelay = true;			RollMode = false;			Luck = Math.random();			AngleRoll = Math.random();			if (Luck <= 0.16666667)			{				Side = 1;			}			else if (Luck <= 0.33333333)			{				Side = 2;			}			else if (Luck <= 0.5)			{				Side = 3;			}			else if (Luck <= 0.66666667)			{				Side = 4;			}			else if (Luck <= 0.83333334)			{				Side = 5;			}			else			{				Side = 6;			}		}	}}